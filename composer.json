{
    "name": "showapp/laravel-chargebee",
    "type": "library",
    "description": "A Laravel package which provides an easy way to handle billing and subscriptions.",
    "keywords": [
        "TijmenWierenga",
        "Chargebee",
        "Laravel",
        "Cashier",
        "Subscription"
    ],
    "homepage": "https://github.com/TijmenWierenga/LaravelChargebee",
    "license": "MIT",
    "authors": [
        {
            "name": "Tijmen Wierenga",
            "email": "tijmen@floown.com",
            "homepage": "https://www.floown.com",
            "role": "Developer"
        },
        {
            "name": "Konrad Marci≈Ñczyk",
            "email": "contact@showapp.pl",
            "homepage": "https://showapp.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "dev-master",
        "illuminate/database": "dev-master",
        "illuminate/console": "dev-master",
        "php" : "~7.2",
        "chargebee/chargebee-php": "^2.7.2",
        "stripe/stripe-php": "^7.27.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "TijmenWierenga\\LaravelChargebee\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TijmenWierenga\\LaravelChargebee\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "../../bin/phpunit --colors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
